<Window x:Class="LuisaKatrinaReyes.RetailApplicationSystem.windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LuisaKatrinaReyes.RetailApplicationSystem.windows"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnRetailUsers" Content="Users" HorizontalAlignment="Left" Margin="31,34,0,0" VerticalAlignment="Top" Width="198" Height="30" FontSize="16" FontWeight="Bold" Click="btnRetailUsers_Click"/>
        <Button x:Name="btnCustomers" Content="Customers" HorizontalAlignment="Left" Margin="31,82,0,0" VerticalAlignment="Top" Width="198" Height="30" FontSize="16" FontWeight="Bold" Click="btnCustomers_Click"/>
        <Button x:Name="btnProducts" Content="Products" HorizontalAlignment="Left" Margin="31,132,0,0" VerticalAlignment="Top" Width="198" Height="30" FontWeight="Bold" FontSize="16" Click="btnProducts_Click"/>
    </Grid>
</Window>
